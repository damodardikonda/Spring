package com.miniproject.module.AttendanceManagementSystem.Entity;

public class AttendanceApplyRequest {

	private String fromdate ;
	private String todate;
	private int count;
	
	int holiday[] = new int[30];

	public String getFromdate() {
		return fromdate;
	}

	public void setFromdate(String fromdate) {
		this.fromdate = fromdate;
	}

	public String getTodate() {
		return todate;
	}

	public void setTodate(String todate) {
		this.todate = todate;
	}

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}

	public int[] getHoliday() {
		return holiday;
	}

	public void setHoliday(int[] holiday) {
		this.holiday = holiday;
	}

	public AttendanceApplyRequest() {}
	public AttendanceApplyRequest(String fromdate, String todate, int count, int[] holiday) {
		super();
		this.fromdate = fromdate;
		this.todate = todate;
		this.count = count;
		this.holiday = holiday;
	}
	
	
}
